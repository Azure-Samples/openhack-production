{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/LinkyLink/LinkyLink.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/src/LinkyLink/LinkyLink.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "${workspaceFolder}/src/LinkyLink/LinkyLink.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    }
      {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/src/LinkyLink/LinkyLink.csproj",
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "clean release",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "--configuration",
        "Release",
        "${workspaceFolder}/src/LinkyLink/LinkyLink.csproj",
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "clean solution",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/LinkyLink.sln"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build tests",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/tests/LinkyLink.Tests/LinkyLink.Tests.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "run tests",
      "command": "dotnet",
      "type": "process",
      "args": [
        "test",
        "${workspaceFolder}/tests/LinkyLink.Tests/LinkyLink.Tests.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
